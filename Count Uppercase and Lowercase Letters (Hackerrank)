#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    char s[1001];
    int uppercase_count = 0;
    int lowercase_count = 0;
    int i;
    if (fgets(s, sizeof(s), stdin) == NULL) {
        return 1;
    }
    s[strcspn(s, "\n")] = '\0';
    for (i = 0; s[i] != '\0'; i++) {
        if (isupper(s[i])) {
            uppercase_count++;
        } else if (islower(s[i])) {
            lowercase_count++;
        }
    }
    printf("%d %d\n", uppercase_count, lowercase_count);
    return 0;
}
