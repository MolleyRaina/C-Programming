#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    char s[1001]; 
    fgets(s, sizeof(s), stdin);
    int vowels = 0;
    int consonants = 0;
    for (int i = 0; s[i] != '\0'; i++) {
        char ch = tolower(s[i]); 
        if (isalpha(ch)) {
            if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
                vowels++;
            } else {
                consonants++;
            }
        }
    }
    printf("%d %d\n", vowels, consonants); 
    return 0;
}
