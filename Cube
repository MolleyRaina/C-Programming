#include<stdio.h>
int main(){
    int num,cube;
    printf("Enter a number");
    scanf("%d",&num);
    if (num % 2 ==0){
        printf("%d is even number\n",num);
    } else{
        printf("%d is odd number\n",num);
    }
    cube=num*num*num;
    printf("Cube of % is %d\n",num,cube);
    return 0;
}
