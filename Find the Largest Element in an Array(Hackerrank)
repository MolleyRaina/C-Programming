#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int N;
    scanf("%d", &N);
    int arr[N];
    int largest_element;
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
        if (i == 0) {
            largest_element = arr[i];
        } else {
            if (arr[i] > largest_element) {
                largest_element = arr[i];
            }
        }
    }
    printf("%d\n", largest_element); 
    return 0;
}
