#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d", &n);
    int arr[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    int max = arr[0];
    for (int i = 1; i < n; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
    }
    int secondMax = -10001;  
    int found = 0;
    for (int i = 0; i < n; i++) {
        if (arr[i] < max && arr[i] > secondMax) {
            secondMax = arr[i];
            found = 1;
        }
    }
    if (found)
        printf("%d", secondMax);
    else
        printf("No second largest element");
    return 0;
}
