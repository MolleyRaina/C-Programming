#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N, reversed = 0, remainder;
    scanf("%d", &N);
    while (N != 0) {
        remainder = N % 10;
        reversed = reversed * 10 + remainder;
        N /= 10;
    }
    printf("%d\n", reversed);
    return 0;
}
