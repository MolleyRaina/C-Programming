#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int R,C,sum=0;
      scanf("%d%d",&R,&C);
    int arr[R][C];
    for(int i=0;i<R;i++){
        for(int j=0;j<C;j++){
            scanf("%d",&arr[i][j]);
        }
    }
    for(int i=0;i<R;i++){
        sum=0;
        for(int j=0;j<C;j++){
            sum = sum + arr[i][j];
        }
    printf("%d\n",sum);
    }
    return 0;
}
