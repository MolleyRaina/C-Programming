#include <stdio.h>
#include <math.h>

int main() {
    int n,fd,ld,digits=0,temp,power;
    scanf("%d",&n);
    
    if(n>=0 && n<=9){
        printf("%d\n",n);
        return 0;
    }
    temp=n;
    ld=n%10;
    
    while(temp>=10){
        temp/=10;
        digits++;
    }
    fd=temp;
    power=pow(10,digits);
    int middlepart=(n%power)/10;
    int newnumber=ld*power+middlepart*10+fd;
    printf("%d\n",newnumber);
    return 0;
}
